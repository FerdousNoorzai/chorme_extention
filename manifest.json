{
    "name": "Youtube Downloader",
    "description": "Download YouTube videos simply than before.",
    "version": "0.1",
    "author": "Abdullah Kaamil", "Ferdous Noorzai",
    "manifest_version": 2,
    "web_accessible_resources": [
        "src/youtubedl.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch*"
            ],
            "js": [
                "src/loader.js"
            ]
        }
    ],
    "permissions": [
        "downloads"
    ]
}
