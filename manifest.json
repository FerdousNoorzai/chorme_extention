{
    "name": "Simple Youtube Dowloader",
    "description": "Download youtube videos simple then before.",
    "version": "0.1",
    "author": "Abdullah Kaamil",
    "manifest_version": 2,
    "web_accessible_resources": [
        "src/youtubedl.js"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch*"
            ],
            "js": [
                "src/loader.js"
            ]
        }
    ]
}